		<!-- Mobile Native -->
		<em:targetApplication>
			<Description>
				<em:id>{aa3c5121-dab2-40e2-81ca-7ea25febc110}</em:id>
				<em:minVersion>40.0</em:minVersion>
				<em:maxVersion>46.*</em:maxVersion>
			</Description>
		</em:targetApplication>
	</Description>
</RDF>