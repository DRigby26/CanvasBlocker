<?xml version="1.0" encoding="utf-8"?>
<RDF xmlns="http://www.w3.org/1999/02/22-rdf-syntax-ns#" xmlns:em="http://www.mozilla.org/2004/em-rdf#">
	<Description about="urn:mozilla:install-manifest">
		<em:id>CanvasBlocker@kkapsner.de</em:id>
		<em:type>2</em:type>
		<em:bootstrap>true</em:bootstrap>
		<em:unpack>false</em:unpack>
		<em:version>0.2.3-Development</em:version>
		<em:name>CanvasBlocker</em:name>
		<em:description>Changes the JS-API for modifying &lt;canvas&gt; to prevent Canvas-Fingerprinting.</em:description>
		<em:creator>Korbinian Kapsner</em:creator>
		<em:homepageURL>https://github.com/kkapsner/CanvasBlocker/</em:homepageURL>
		<em:optionsURL>data:text/xml,&lt;placeholder/&gt;</em:optionsURL>
		<em:optionsType>2</em:optionsType>
		
